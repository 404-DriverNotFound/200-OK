server
{
	host 127.0.0.1
	port 8000
	timeout 200
	cgi_extension .bla
	index_pages index.html
	location /put_test
	{
		root YoupiBanane/directory
	}
	location /post_body
	{
		client_max_body_size 100
		root YoupiBanane/directory
		autoindex on
	}
	location /directory
	{
		root YoupiBanane
		index_pages youpi.bad_extension
	}
	location /html
	{
		auth_basic_user_file .htpasswd
		autoindex on
	}
}