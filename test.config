server
{
	port 8000
	timeout 200
	cgi_extension .bla
	index_pages index.html
	method GET
	location /put_test
	{
		root YoupiBanane/directory
		method PUT
	}
	location /post_body
	{
		method POST
		root YoupiBanane/directory
		index_pages temp
	}
	location /directory
	{
		method GET POST
		root YoupiBanane
		index_pages youpi.bad_extension
	}
}